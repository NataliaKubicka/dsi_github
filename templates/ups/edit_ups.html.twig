{% include 'dashboard/index.html.twig' %}


{% block body %}
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
    <div class="w-75 mx-auto custom-card my-3 px-3 py-5">
        <div class="form-group">
            {{ form_row(form.city) }}
        </div>
        <div class="form-group">
            {{ form_row(form.address) }}
        </div>
        <div class="form-group">
            {{ form_row(form.localization) }}
        </div>
        <div class="form-group">
            {{ form_row(form.assignment) }}
        </div>
        <div class="form-group">
            {{ form_row(form.ip) }}
        </div>
        <div class="form-group">
            {{ form_row(form.model) }}
        </div>
        <div class="form-group">
            {{ form_row(form.sn) }}
        </div>
        <div class="form-group">
            {{ form_row(form.info) }}
        </div>
        <div class="form-group">
            {{ form_row(form.save, {'label' : 'Aktualizuj'}) }}
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}