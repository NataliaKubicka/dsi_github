{% include 'dashboard/index.html.twig' %}


{% block body %}
    {{ form_start(form) }}
    <div class="w-75 mx-auto custom-card my-3 px-3 py-5">
        <div class="form-group">
            {{ form_row(form.username) }}
        </div>
        <div class="form-group">
            {{ form_row(form.surname) }}
        </div>
        <div class="form-group">
            {{ form_row(form.search) }}
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}

